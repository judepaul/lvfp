    <div class="container">
      <%#= flash[:alert] %>
      <%#= flash[:notice] %>
      <div class="container">
  <% flash.each do |type, msg| %>
    <div style="background-color: #DCDCDC!important;border-color: #DCDCDC!important;" class="alert <%= bootstrap_class_for_flash(type) %> alert-dismissable fade show">
      <%= msg %>
      <button class="close" data-dismiss="alert">x</button>
    </div>
  <% end %> 
</div>